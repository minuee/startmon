package startMon.Status;

public class IpStatusConfig {
    private String ip;
    private int idx;
    private String service_status;
    private String rack;
    private String sw;
    private String port;
    private boolean monitoring_check;
    private int calll;
    private String check_port_no;
    private boolean check_ICMP;
    private boolean check_pv_ICMP;
    private String ip_pv;
    private String pct;
    private String relay;
    private String down_port_no;
    private String domain;
    private int load_cnt;

    public int getLoad_cnt() {
        return load_cnt;
    }

    public void setLoad_cnt(int load_cnt) {
        this.load_cnt = load_cnt;
    }

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }

    public int getIdx() {
        return idx;
    }

    public void setIdx(int idx) {
        this.idx = idx;
    }

    public String getService_status() {
        return service_status;
    }

    public void setService_status(String service_status) {
        this.service_status = service_status;
    }

    public String getRack() {
        return rack;
    }

    public void setRack(String rack) {
        this.rack = rack;
    }

    public String getSw() {
        return sw;
    }

    public void setSw(String sw) {
        this.sw = sw;
    }

    public String getPort() {
        return port;
    }

    public void setPort(String port) {
        this.port = port;
    }

    public boolean getMonitoring_check() {
        return monitoring_check;
    }

    public void setMonitoring_check(boolean monitoring_check) {
        this.monitoring_check = monitoring_check;
    }

    public int getCalll() {
        return calll;
    }

    public void setCalll(int calll) {
        this.calll = calll;
    }

    public String getCheck_port_no() {
        return check_port_no;
    }

    public void setCheck_port_no(String check_port_no) {
        this.check_port_no = check_port_no;
    }

    public boolean getCheck_ICMP() {
        return check_ICMP;
    }

    public void setCheck_ICMP(boolean check_ICMP) {
        this.check_ICMP = check_ICMP;
    }

    public boolean getCheck_pv_ICMP() {
        return check_pv_ICMP;
    }

    public void setCheck_pv_ICMP(boolean check_pv_ICMP) {
        this.check_pv_ICMP = check_pv_ICMP;
    }

    public String getIp_pv() {
        return ip_pv;
    }

    public void setIp_pv(String ip_pv) {
        this.ip_pv = ip_pv;
    }

    public String getPct() {
        return pct;
    }

    public void setPct(String pct) {
        this.pct = pct;
    }

    public String getRelay() {
        return relay;
    }

    public void setRelay(String relay) {
        this.relay = relay;
    }

    public String getDown_port_no() {
        return down_port_no;
    }

    public void setDown_port_no(String down_port_no) {
        if (down_port_no == null) {
            this.down_port_no = "123456789";
        }
        this.down_port_no = down_port_no;
    }

    public String getDomain() {
        return domain;
    }

    public void setDomain(String domain) {
        this.domain = domain;
    }

}
